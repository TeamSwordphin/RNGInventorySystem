{
  "name": "RNGInventorySystem",
  "emitLegacyScripts": false,

  "tree": {
    "$className": "DataModel",

    "ReplicatedStorage": {
      "Modules": {
        "$path": "src/shared"
      },
      "Libraries":{
        "$path": "Packages",
        "DynamicDepth": {
          "$path": "src/libraries/DynamicDepth.luau"
        },
        "ReplicaController":{
          "$path": "src/libraries/ReplicaController.luau"
        }
      },
      "ClientSystems": {
        "$path": "src/client"
      },
      "Network": {
        "$path": "src/zap/src/client"
      }
    },

    "ServerScriptService": {
      "ServerSystems": {
        "$path": "src/server"
      },
      "Network": {
        "$path": "src/zap/src/server"
      }
    },

    "Workspace": {
      "$properties": {
        "FilteringEnabled": true
      },
      "Baseplate": {
        "$className": "Part",
        "$properties": {
          "Anchored": true,
          "Color": [
            0.38823,
            0.37254,
            0.38823
          ],
          "Locked": true,
          "Position": [
            0,
            -10,
            0
          ],
          "Size": [
            512,
            20,
            512
          ]
        }
      }
    },
    "Lighting": {
      "$properties": {
        "Ambient": [
          0,
          0,
          0
        ],
        "Brightness": 2,
        "GlobalShadows": true,
        "Outlines": false,
        "Technology": "Voxel"
      }
    },
    "SoundService": {
      "$properties": {
        "RespectFilteringEnabled": true
      }
    }
  }
}